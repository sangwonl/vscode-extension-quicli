{
  "name": "quicli",
  "displayName": "QuiCLI",
  "description": "Quickly run command line written on editor.",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/sangwonl/vscode-extension-quicli.git"
  },
  "publisher": "gamz",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quicli.runCommandInTerminal",
        "category": "QuiCLI",
        "title": "Run Command in Terminal"
      }
    ]
  },
  "lint-staged": {
    "**/*": [
      "eslint src --ext ts",
      "prettier --write '**/*.{ts,js}' --ignore-unknown"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "tsc -p ./",
    "login": "vsce login gamz",
    "package": "vsce package",
    "publish": "vsce publish",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "format": "prettier --write '**/*.{ts,js}' --ignore-unknown"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.20.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "prettier": "3.0.1",
    "typescript": "^5.1.3"
  }
}
